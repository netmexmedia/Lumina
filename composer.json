{
    "name": "netmex/lumina",
    "description": "LuminaBundle: A Symfony bundle providing Lighthouse-inspired GraphQL support with automatic Doctrine integration, custom resolvers, scalars, directives, queries, and mutations.",
    "type": "symfony-bundle",
    "require": {
        "symfony/framework-bundle": "^7.3",
        "doctrine/orm": "^3.5",
        "webonyx/graphql-php": "^15.29",
        "symfony/dependency-injection": "^7.3",
        "symfony/event-dispatcher": "^7.3",
        "symfony/config": "^7.3",
        "symfony/property-access": "^7.4",
        "symfony/http-kernel": "^7.3",
        "symfony/serializer-pack": "^1.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Netmex\\Lumina\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Danny",
            "email": "d.verhoeven@netmex.nl"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^12.5"
    }
}
